
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>SmartPhoneShop</title>

    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Hind:400,700" rel="stylesheet">

    <!-- Bootstrap -->
    <link href="~/Assets/client/css/bootstrap.min.css" rel="stylesheet" />
    <link type="text/css" rel="stylesheet" href="~/Assets/client/css/bootstrap.min.css" />

    <!-- Slick -->
    <link type="text/css" rel="stylesheet" href="~/Assets/client/css/slick.css" />
    <link type="text/css" rel="stylesheet" href="~/Assets/client/css/slick-theme.css" />

    <!-- nouislider -->
    <link type="text/css" rel="stylesheet" href="~/Assets/client/css/nouislider.min.css" />

    <!-- Font Awesome Icon -->
    <link rel="stylesheet" href="~/Assets/client/css/font-awesome.min.css">

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="~/Assets/client/css/style.css" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    <link href="~/Assets/client/css/templateContain.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/jquery-ui/themes/smoothness/jquery-ui.min.css" rel="stylesheet" />
</head>

<body ng-app="client">
    <!-- HEADER -->
    @{ Html.RenderAction("header", "Home");}
    <!-- /HEADER -->
    @{ Html.RenderAction("navigation", "Home");}
    <!-- NAVIGATION -->
    <!-- /NAVIGATION -->
    <!-- HOME -->
    @RenderBody()

    @{ Html.RenderAction("footer", "Home");}
    <!-- jQuery Plugins -->
    <script src="~/Assets/client/js/jquery.min.js"></script>
    <script src="~/Assets/admin/libs/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Assets/client/js/bootstrap.min.js"></script>
    <script src="~/Assets/client/js/slick.min.js"></script>
    <script src="~/Assets/client/js/nouislider.min.js"></script>
    <script src="~/Assets/client/js/jquery.zoom.min.js"></script>
    <script src="~/Assets/client/js/main.js"></script>
    <script src="~/Assets/admin/libs/mustache/mustache.js"></script>
    <script src="~/Assets/admin/libs/angular/angular.min.js"></script>
    <script src="~/Assets/client/appClient.js"></script>
    <script src="~/Assets/client/js/common.js"></script>
    <script src="~/Assets/admin/libs/numeral/numeral.js"></script>
    @RenderSection("footerJS", required: false)
